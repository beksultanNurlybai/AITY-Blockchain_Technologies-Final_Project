<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Marketplace</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
    <header class="bg-dark text-white text-center py-4">
        <h1>Marketplace</h1>
        <nav>
            <a class="text-white mr-4" href="/register">Register</a>
            <a class="text-white" href="/login">Login</a>
        </nav>
    </header>

    <div class="container my-4">
        <div class="row">
            <!-- Card Example 1 -->
            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">CPU Name: Intel i9</h5>
                        <p class="card-text">CPU Count: 8</p>
                        <p class="card-text">RAM Size: 32GB</p>
                        <p class="card-text">Owner: 0x123456789abcdef</p>
                    </div>
                </div>
            </div>
            <!-- Card Example 2 -->
            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">CPU Name: AMD Ryzen 7</h5>
                        <p class="card-text">CPU Count: 8</p>
                        <p class="card-text">RAM Size: 16GB</p>
                    </div>
                </div>
            </div>
            <!-- Card Example 3 -->
            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">CPU Name: Intel i5</h5>
                        <p class="card-text">CPU Count: 6</p>
                        <p class="card-text">RAM Size: 8GB</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.3/dist/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
</html>
